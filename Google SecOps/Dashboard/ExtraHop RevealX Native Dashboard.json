            "dashboardQueries": [
                {
                    "name": "9ce5d32e-7427-40e6-9dc8-4b5001588b67",
                    "query": "metadata.log_type=\"EXTRAHOP\"\r\nsecurity_result.priority = \"HIGH_PRIORITY\"\r\noutcome:\r\n    $recommended_detection_events = count_distinct(metadata.id)\r\n",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "DAY",
                            "startTimeVal": "7"
                        }
                    },
                    "etag": "240458121c7a5828ee814288272acdcd0ba7222cc361d6aa9ab38fd024817c16"
                },
                {
                    "name": "a77357de-b536-4e55-b331-bf34b7ec4f9f",
                    "query": "metadata.log_type = \"EXTRAHOP\"\r\n$hostname = if(principal.hostname = \"\", if(principal.ip = \"\", if(principal.resource.attribute.labels[\"src_device_name\"] = \"\", \"-\", principal.resource.attribute.labels[\"src_device_name\"]), principal.ip), principal.hostname)\r\n$hostname != \"-\"\r\nmatch:\r\n    $hostname\r\noutcome:\r\n    $count = count_distinct(metadata.id)\r\norder:\r\n    $count desc\r\nlimit:\r\n    10",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "HOUR",
                            "startTimeVal": "24"
                        }
                    },
                    "etag": "2b314fcee92aec55f7a73ad023a2d7dac3e9f38d0f65a2d10ba10211fb22483a"
                },
                {
                    "name": "62a38e2f-9547-4d52-9312-fe07a4f3b546",
                    "query": "metadata.log_type = \"EXTRAHOP\"\r\n$hostname = if(target.hostname = \"\", if(target.ip = \"\", if(target.resource.attribute.labels[\"dst_device_name\"] = \"\", \"-\", target.resource.attribute.labels[\"src_device_name\"]), target.ip), target.hostname)\r\n$hostname != \"-\"\r\nmatch:\r\n    $hostname\r\noutcome:\r\n    $count = count_distinct(metadata.id)\r\norder:\r\n    $count desc\r\nlimit:\r\n    10",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "HOUR",
                            "startTimeVal": "24"
                        }
                    },
                    "etag": "311e2dd939f28bebd38702bbc224ff3fdd943dac1c6d8f3cb9540577a7561524"
                },
                {
                    "name": "67d8e0f6-1041-4266-9a88-aef08d760fb3",
                    "query": "metadata.log_type = \"EXTRAHOP\"\r\ntarget.ip_geo_artifact.ip != \"\"\r\n$lat = target.ip_geo_artifact.location.region_coordinates.latitude\r\n$long = target.ip_geo_artifact.location.region_coordinates.longitude\r\n$target_ip = target.ip_geo_artifact.ip\r\nmatch:\r\n    $target_ip, $lat, $long\r\noutcome:\r\n    $count = count($target_ip)\r\norder:\r\n    $count desc",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "HOUR",
                            "startTimeVal": "24"
                        }
                    },
                    "etag": "cafcdf6c0ab94dbb61eea44ebf8fe68c4e3e4a3e3bc9edb2c207737e2e4263a5"
                },
                {
                    "name": "83935668-090e-411e-bfb0-220768086d54",
